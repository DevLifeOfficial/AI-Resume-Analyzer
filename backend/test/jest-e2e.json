{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testEnvironment": "node",
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
    "collectCoverageFrom": [
    "../**/*.(t|js)",
    "!../**/*.spec.ts",
    "!../**/*.e2e-spec.ts",
    "!../**/*.d.ts"
  ]
}
